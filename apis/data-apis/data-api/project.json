{
  "name": "data-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apis/data-apis/data-apis/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apis/data-apis/data-apis",
        "main": "apis/data-apis/data-apis/src/main.ts",
        "tsConfig": "apis/data-apis/data-apis/tsconfig.app.json",
        "assets": ["apis/data-apis/data-apis/src/assets"],
        "webpackConfig": "apis/data-apis/data-apis/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "data-api:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "data-api:build:development"
        },
        "production": {
          "buildTarget": "data-api:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apis/data-apis/data-apis/jest.config.ts"
      }
    }
  },
  "tags": []
}
